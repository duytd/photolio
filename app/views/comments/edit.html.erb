<%= form_for( @comment, url: content_comment_path(@comment), method: :put ) do |f| %>
  <%#= render 'shared/error_messages', object: f.object %>
  <div class="edit_wrap">
    <%= f.label :subject %>
    <%= f.text_field :subject %>
    <%= f.label :com_content %>
    <%= f.text_area :com_content %>
  </div>
  <div class='edit_btn_wrap'>
    <%= f.submit "수 정", class: "btn btn-middle btn-primary" %>
  </div>
<% end %>
